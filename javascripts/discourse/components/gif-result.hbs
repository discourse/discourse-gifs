<div
  {{on "click" (fn @pick @gif)}}
  {{on "keydown" this.keyDown}}
  role="button"
  tabindex="0"
  class="gif-imgwrap"
>
  <img
    class="gif-img"
    alt={{@gif.title}}
    title={{@gif.title}}
    src={{@gif.preview}}
    style={{this.style}}
    width={{@gif.width}}
    height={{@gif.height}}
  />
</div>